<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="user">
       
    <select id="getUserList"  resultClass="com.wtu.tbdata.domain.User">
		SELECT 
			id,username,password
		FROM
			user_admin	 	
    </select>
       
    <insert id="insertUser" parameterClass="com.wtu.tbdata.domain.User">
    	insert into user_admin
    	(
    	  id,
    	  username,
    	  password
    	)
    	values(
          #id#,
    	  #username#,
    	  #password#
       	)
    </insert>
</sqlMap>